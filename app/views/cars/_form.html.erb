<%= form_with(model: car, local: true) do |form| %>
<%= render 'shared/errors', object: car %>

<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, :class=>'form-control' %>
</div>

<div class="form-group">
  <%= form.submit :class => 'btn btn-primary' %>
</div>
<% end %>
